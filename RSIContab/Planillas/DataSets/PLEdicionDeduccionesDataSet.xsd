                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     PLDeducciones.NoDocumento, PLDeducciones.Fecha, PLDeducciones.CodigoDeduccion, PLDeducciones.CodigoEmpleado, 
                      PLDeducciones.PlanillaNo, PLDeducciones.Valor, PLDeducciones.NoMesesDeducir, PLDeducciones.DeduccionMensual, 
                      PLDeducciones.DeduccionPermanente, PLDeducciones.MontoDeducido, PLDeducciones.NoPlanillasDeducidas, PLDeducciones.UltimaDeduccion, 
                      PLDeducciones.Referencia, PLDeducciones.OtrosCreditos, PLDeducciones.DeduccionPlanilla1, PLDeducciones.DeduccionPlanilla2, 
                      PLDeducciones.DeduccionPlanilla3, PLDeducciones.DeduccionPlanilla4, PLDeducciones.Año, PLDeducciones.Mes, PLDeducciones.Anulada, (ISNULL(PLEmpleados.Nombre1,'') +' ' + ISNULL(PLEmpleados.Nombre2,'') + ' ' + ISNULL(PLEmpleados.Apellido1,'')+' ' + ISNULL(PLEmpleados.Apellido2,'')) AS Nombre, (PLDeducciones.Valor-PLDeducciones.MontoDeducido) AS Saldo 
FROM         PLDeducciones INNER JOIN
                      PLEmpleados ON PLDeducciones.CodigoEmpleado = PLEmpleados.CodigoEmpleado
WHERE     (PLDeducciones.CodigoDeduccion = @CodigoDeduccion) AND (PLDeducciones.Anulada = 0) AND (PLDeducciones.DeduccionPermanente = 1) OR
                      (PLDeducciones.CodigoDeduccion = @CodigoDeduccion) AND (PLDeducciones.Anulada = 0) AND 
                      (PLDeducciones.MontoDeducido &lt; PLDeducciones.Valor)
ORDER BY PLDeducciones.CodigoDeduccion, PLDeducciones.Año, PLDeducciones.Mes, PLEmpleados.Nombre1, PLEmpleados.Nombre2, 
                      PLEmpleados.Apellido1, PLEmpleados.Apellido2</CommandText>
